			"name": "Register",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "9eab26da-4d75-4ed8-b4c7-8c52df9070c4",
						"exec": [
							"// Check if the response status code is 200\r",
							"pm.test(\"Status code is 201\", function () {\r",
							"    pm.expect(pm.response.code).to.equal(200);\r",
							"});\r",
							"\r",
							"// Check if the response time is less than 200ms\r",
							"pm.test(\"Response time is less than 200ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(200);\r",
							"});\r",
							"\r",
							"// Check if the response body contains the expected message\r",
							"pm.test(\"Response body contains the expected message\", function () {\r",
							"    const response = pm.response.json();\r",
							"    pm.expect(response.message).to.exist;\r",
							"    pm.expect(response.message).to.be.a('string');\r",
							"    pm.expect(response.message).to.equal(\"Test route working\");\r",
							"});\r",
							"\r",
							"// Check if the Content-Type header is set to application/json\r",
							"pm.test(\"Content-Type is application/json\", function () {\r",
							"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/json\");\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
